Photo, saveProfile}),\r\n    withRouter\r\n)(ProfileContainer);","D:\\Programming_training\\react-kabzda-kak-prosto\\projects\\01-first-project\\react-kabzda-1\\src\\redux\\auth-reducer.js",[],"D:\\Programming_training\\react-kabzda-kak-prosto\\projects\\01-first-project\\react-kabzda-1\\src\\components\\Header\\HeaderContainer.jsx",[],"D:\\Programming_training\\react-kabzda-kak-prosto\\projects\\01-first-project\\react-kabzda-1\\src\\Api\\Api.js",[],"D:\\Programming_training\\react-kabzda-kak-prosto\\projects\\01-first-project\\react-kabzda-1\\src\\hoc\\withAuthRedirect.js",[],"D:\\Programming_training\\react-kabzda-kak-prosto\\projects\\01-first-project\\react-kabzda-1\\src\\components\\Login\\Login.jsx",["205"],"import React from 'react';\r\nimport {reduxForm} from 'redux-form';\r\nimport {Input, createField} from '../common/FormsControls/FormsControls';\r\nimport {required} from '../../utils/validators/validators';\r\nimport {connect} from 'react-redux';\r\nimport {login} from '../../redux/auth-reducer';\r\nimport { Redirect } from 'react-router-dom';\r\nimport styles from '../common/FormsControls/FormsControls.module.css';\r\n\r\nconst LoginForm = ({handleSubmit, error, captchaUrl}) => {\r\n\r\n    return (\r\n        <form onSubmit={handleSubmit}>\r\n            { createField(\"Email\", \"email\", [required], Input) }\r\n            { createField(\"Password\", \"password\", [required], Input, {type: \"password\"}) }\r\n            { createField(null, \"rememberMe\", [], Input, {type: \"checkbox\"}, \"remember me\") }\r\n            \r\n            {captchaUrl && <img src={captchaUrl} />}\r\n            {captchaUrl && createField(\"Symbols from image\", \"captcha\", [required], Input, {}) }\r\n\r\n            {error && <div className={styles.formSummaryError}>{error}</div>}\r\n            <div>\r\n                <button>Login</button>\r\n            </div>\r\n        </form>\r\n    )\r\n}\r\n\r\nconst LoginReduxForm = reduxForm({form: 'login'})(LoginForm)\r\n\